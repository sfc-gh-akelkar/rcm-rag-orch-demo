# Snowflake Secrets Configuration
# Copy this file to secrets.toml and fill in your actual credentials
# NEVER commit secrets.toml to version control!

[snowflake]
user = "your_snowflake_user"
password = "your_snowflake_password"
account = "your_snowflake_account"  # Format: orgname-accountname
warehouse = "RCM_INTELLIGENCE_WH"
database = "RCM_AI_DEMO"
schema = "RCM_SCHEMA"
role = "SF_INTELLIGENCE_DEMO"

